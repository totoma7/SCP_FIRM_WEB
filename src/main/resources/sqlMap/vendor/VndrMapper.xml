<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="VndrMapper">

    <select id="selectVndrList" parameterType="java.util.HashMap" resultType="java.util.HashMap">
    SELECT USER_ID
         , USER_NM
         , USER_EMAIL
         , PWD
         , USE_YN
         , FAIL_DT
         , FAIL_CNT
         , LOCK_YN
      FROM TB_USER_INFO
     WHERE 1 = 1
    </select>


</mapper>
